{
    "ansible.ansible.reuseTerminal": true,
    "ansible.executionEnvironment.containerEngine": "docker",
    "ansible.executionEnvironment.enabled": true,
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "C_Cpp.intelliSenseEngine": "disabled",
    "cmake.automaticReconfigure": false,
    "cmake.buildDirectory": "${workspaceFolder}/cmake_build",
    "cmake.configureOnEdit": false,
    "cmake.configureOnOpen": false,
    "diffEditor.codeLens": true,
    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.experimental.showMoves": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.autoClosingQuotes": "languageDefined",
    "editor.autoIndent": "full",
    "editor.autoSurround": "languageDefined",
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.fontLigatures": true,
    "editor.fontFamily": "'JetBrains Mono', 'monospace', monospace, 'Hack Nerd Font'",
    "editor.renderWhitespace": "all",
    "editor.semanticHighlighting.enabled": true,
    "files.autoSave": "afterDelay",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.enableCommitSigning": true,
    "git.mergeEditor": true,
    "github.gitProtocol": "ssh",
    "go.diagnostic.vulncheck": "Imports",
    "makefile.configureAfterCommand": false,
    "makefile.configureOnEdit": false,
    "makefile.configureOnOpen": false,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "remote.downloadExtensionsLocally": true,
    "remote.SSH.configFile": "~/.ssh/config",
    "remote.tunnels.access.preventSleep": true,
    "scm.alwaysShowRepositories": true,
    "telemetry.telemetryLevel": "error",
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.accessibleViewFocusOnCommandExecution": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.minimumContrastRatio": 1,
    "typst-lsp.exportPdf": "never",
    "typst-lsp.experimentalFormatterMode": "on",
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.iconTheme": "catppuccin-mocha",
    "workbench.settings.editor": "json",
}